{
  "compilerOptions": {
    // General
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "target": "ESNext",
    // Modules
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    // Type Checking & Safety
    "strict": true,
    "types": [
      "vite/client"
    ]
  },
  // "include": [
  //   "energy/rates.json"
  // ]
}